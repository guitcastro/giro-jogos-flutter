# Firebase Configuration
# Copy this file to .env and fill in your Firebase project details
# These values can be obtained from your Firebase project settings

# Project ID
FIREBASE_PROJECT_ID=your-project-id

# Web API Key
FIREBASE_WEB_API_KEY=your-web-api-key

# Android API Key  
FIREBASE_ANDROID_API_KEY=your-android-api-key

# iOS API Key
FIREBASE_IOS_API_KEY=your-ios-api-key

# App IDs
FIREBASE_WEB_APP_ID=your-web-app-id
FIREBASE_ANDROID_APP_ID=your-android-app-id
FIREBASE_IOS_APP_ID=your-ios-app-id

# Messaging Sender ID
FIREBASE_MESSAGING_SENDER_ID=your-messaging-sender-id

# iOS Bundle ID
IOS_BUNDLE_ID=com.example.giroJogos
